{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "key-spacing": [2, {"align": "colon"}],
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "react/require-default-props": [0, {"forbidDefaultForRequired": false}],
    "react/prop-types": [
      2,
      {
        "ignore": [
          "dispatch", //
          "children", //
          "form"      // supported by redux-form
        ]
      }
    ]
  }
}
